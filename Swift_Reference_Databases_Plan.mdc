@main
struct SkillTalkApp: App {
    init() {
        // Setup dependency injection
        ServiceContainer.shared.registerReferenceDataServices()
    }
    
    var body: some Scene {
        WindowGroup {
            ContentView()
                .environmentObject(
                    ServiceContainer.shared.resolve(ReferenceDataManagerProtocol.self)!
                )
        }
    }
}
